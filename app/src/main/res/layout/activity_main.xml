<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">
<androidx.appcompat.widget.Toolbar
    android:background="@color/colorPrimaryDark"
    android:id="@+id/toolbarclub"
    app:titleTextColor="#fff"
    app:title="Taufan Ryanda"
    app:subtitleTextColor="#fff"
    app:subtitle="http://github.com/ryanda-utama"
    app:logo="@android:drawable/ic_search_category_default"
    android:layout_width="match_parent"
    android:layout_height="56dp"></androidx.appcompat.widget.Toolbar>
<ListView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/listview"
    android:layout_marginStart="16dp"
    ></ListView>
</LinearLayout>